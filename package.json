	"scripts": {
		"anz": "cross-env ANALYZE=1 next build client && cat build/.next/stats.txt",
		"sty": "stylelint ./client/**/*.js",
		"test": "clear & NODE_ENV=test jest",
		"debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
		"story": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"dev": "NODE_ENV=production next client",
		"next:build": "NODE_ENV=development next build client",
		"server:build": "NODE_ENV=server babel server -d build/dist",
		"prebuild": "rm -rf build/*",
		"build": "npm run next:build & npm run server:build",
		"predeploy": "now rm imagine-engine-project",
		"deploy": "now --public",
		"server": "NODE_ENV=test babel-node server/index ",
		"prestart": "NODE_ENV=production next build client",
		"start": "NODE_ENV=production node server",
		"now-start": "NODE_ENV=production node server",
		"reset": "rm -rf node_modules/ & yarn cache clean & yarn install"
	},
	"engines": {
		"node": "9.4.0 || 8.9.x"
	},
	"now": {
		"engines": {
			"node": "8.9.x"
		}
